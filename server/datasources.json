{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "az-api": {
    "name": "az-api",
    "connector": "rest",
    "baseUrl": "http://localhost:8711/api/v1",
    "options": {
      "headers": {
        "accepts": "application/json",
        "content-type": "application/json",
        "user-agent": "az-frontend-node-agent"
      }
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "/published_news_articles/{id}",
          "query": {
            "id": "{id:number}"
          },
          "repsonsePath": "$//*"
        },
        "functions": {
          "getPublishedNewsArticle": [
            "id"
          ]
        }
      }
    ]
  }
}
