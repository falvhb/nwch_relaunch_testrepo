{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "az-api": {
    "name": "az-api",
    "connector": "rest",
    "baseUrl": "http://localhost:8711/api/v1/content",
    "options": {
      "headers": {
        "accepts": "application/json",
        "content-type": "application/json",
        "user-agent": "az-frontend-node-agent"
      }
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "/news_ressorts/{id}"
        },
        "functions": {
          "getNewsRessort": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/news_ressorts",
          "query": {
            "path": "{path}"
          }
        },
        "functions": {
          "queryNewsRessort": [
            "path"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/published_news_articles/{id}"
        },
        "functions": {
          "getPublishedNewsArticle": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/published_news_articles",
          "query": {
            "keywords": "{keywords}",
            "domain": "{domain=aaz}",
            "limit": "{limit=10}",
            "offset": "{offset=0}"
          }
        },
        "functions": {
          "queryPublishedNewsArticle": [
            "keywords",
            "domain",
            "limit",
            "offset"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_audios/{id}"
        },
        "functions": {
          "getAssetAudio": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_documents/{id}"
        },
        "functions": {
          "getAssetDocument": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_html_snippets/{id}"
        },
        "functions": {
          "getAssetHtmlSnippet": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_image_galleries/{id}"
        },
        "functions": {
          "getAssetGallery": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_images/{id}"
        },
        "functions": {
          "getAssetImage": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_quiz/{id}"
        },
        "functions": {
          "getAssetQuiz": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_surveys/{id}"
        },
        "functions": {
          "getAssetSurvey": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/asset_videos/{id}"
        },
        "functions": {
          "getAssetVideo": [
            "id"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "/domains/{id}"
        },
        "functions": {
          "getDomain": [
            "id"
          ]
        }
      }
    ]
  }
}
